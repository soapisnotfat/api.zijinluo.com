{
  "name": "data-server",
  "version": "0.0.1",
  "description": "personal data server",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "./node_modules/nodemon/bin/nodemon.js --exec ts-node src/server.ts",
    "start": "node ./dist/server.js",
    "prod": "npm run build && npm run start",
    "test": "jest"
  },
  "author": "Zijin Luo",
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|tsx|js|jsx)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-graphql": "^0.9.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "axios": "^0.19.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
